  "36da6b7c-3de9-4e81-af59-e932ed7d4cbd": {
    "id": "36da6b7c-3de9-4e81-af59-e932ed7d4cbd",
    "class": "WhileCondition",
    "name": "HThrustHold",
    "assetPack": "core",
    "options": {
      "name": "HThrustHold",
      "init": [
        "() => {",
        "          this.notepad.HThrustStart = Date.now();",
        "        }"
      ],
      "conditional": [
        "() => {",
        "          let hold = false;",
        "          const elapsed = Date.now() - this.notepad.HThrustStart;",
        "",
        "          if (elapsed < 50) {",
        "            hold = true;",
        "          }",
        "",
        "          return hold;",
        "        }"
      ]
    }
  },
